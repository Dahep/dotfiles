# Personal GitHub
Host personalgit
	HostName github.com
	User git
	# IdentityFile ~/.ssh/github-personal.pub
	# IdentitiesOnly yes
	IdentityAgent ~/.1password/agent.sock

Host *
    IdentityAgent ~/.1password/agent.sock
    # IdentityFile ~/.ssh/github-addval.pub
    # IdentitiesOnly yes
    # AddKeysToAgent yes
    ForwardAgent yes
